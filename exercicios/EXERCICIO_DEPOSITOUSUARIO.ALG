Algoritmo "depositousuario"

Var ValorInicial, ValorDeposito: real

Inicio
      ValorInicial<-10000
      escreva("Digite o valor a depósitar: ")
      leia(ValorDeposito)
      escreval("Saldo Inicial:", ValorInicial)
      escreval("Valor do depósito: ", ValorDeposito)
      escreval("Saldo Atual: ", (ValorInicial+ValorDeposito))


Fimalgoritmo