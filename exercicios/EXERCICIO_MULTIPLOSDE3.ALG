Algoritmo "multiplosde3"

Var
   m1 , m2: inteiro;


Inicio
      escreva("Digite o 1ª número: ")
        leia(m1)
      escreva("Digite o 2ª número: ")
        leia(m2)
      se(m1 mod 3)e(m2 mod 3) = 0 entao
        escreva("Os números são multiplos de 3 ")
      senao
      se(m1 <> 3)e(m2 <> 3) = 0 então
        escreva("Os números não são multiplos de 3 ")
      fimse
fimse


Fimalgoritmo