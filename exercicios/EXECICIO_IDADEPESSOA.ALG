Algoritmo "idadepessoa"

Var idade: inteiro;


Inicio
      escreva("Informe sua idade: ")
      leia(idade)
      se(idade<18)entao
      escreval(idade, " Você é menor de idade")
senao
     se(idade>18)e(idade<=60)entao
     escreval(idade, " Você é adulto")
senao
     se(idade>60)entao
     escreval(idade, " Você é idoso")
fimse
fimse fimse
fimse


Fimalgoritmo