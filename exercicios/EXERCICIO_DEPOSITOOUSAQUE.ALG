Algoritmo "depositoousaque"

Var valorsaque, saldo: real;


Inicio
      saldo<-1200
      escreva("Digite o valor do saque: ")
      leia(valorsaque)
         escreval("Saldo inicial: ", saldo)
         escreval("Valor do saque: ", valorsaque)
         escreval("Saldo atual: ", (saldo-valorsaque))


Fimalgoritmo